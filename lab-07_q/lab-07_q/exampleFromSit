 .timescale 0 0;
v0x556699d1d350_0 .var "T", 63 0;
v0x556699d1d7b0_0 .var "data", 3 0;
v0x556699d1dba0_0 .var/i "i_value", 31 0;
v0x556699d65d20_0 .var/real "r_value", 0 0;
    .scope S_0x556699d1bdc0;
T_0 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x556699d1d7b0_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x556699d1bdc0;
T_1 ;
    %vpi_call 2 8 "$monitor", "At time T = %0t: data = %0d, r_value = %0f, i_value = %0h", v0x556699d1d350_0, v0x556699d1d7b0_0, v0x556699d65d20_0, v0x556699d1dba0_0 {0 0 0};
    %pushi/real 1685774663, 4067; load=3.14000
    %pushi/real 2852127, 4045; load=3.14000
    %add/wr;
    %assign/wr v0x556699d65d20_0, 0;
    %pushi/vec4 4, 0, 32;
    %assign/vec4 v0x556699d1dba0_0, 0;
    %delay 2, 0;
    %pushi/vec4 5, 0, 4;
    %assign/vec4 v0x556699d1d7b0_0, 0;
    %pushi/vec4 7, 0, 4;
    %assign/vec4 v0x556699d1d7b0_0, 3;
    %pushi/vec4 10, 0, 32;
    %assign/vec4 v0x556699d1dba0_0, 0;
    %pushi/vec4 6, 0, 32;
    %assign/vec4 v0x556699d1dba0_0, 0;
    %delay 4, 0;
    %vpi_call 2 48 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x556699d1bdc0;
T_2 ;
    %delay 1, 0;
    %vpi_func 2 59 "$time" 64 {0 0 0};
    %store/vec4 v0x556699d1d350_0, 0, 64;
    %jmp T_2;
    .thread T_2;
